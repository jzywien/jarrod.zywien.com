{
  "name": "www.zywien.com",
  "version": "1.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start:server": "node server/app.js",
    "start:client": "cd client && yarn start",
    "start": "concurrently \"yarn start:server\" \"yarn start:client\"",
    "build": "cd client && yarn build:prod && cd ..",
    "deploy": "yarn build && yarn deploy:gcloud",
    "deploy:gcloud": "gcloud app deploy --project jzywien"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@angular/cli": "^6.0.8",
    "concurrently": "^3.6.0"
  }
}
